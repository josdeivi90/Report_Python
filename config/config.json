    "az_availability_metrics": [
        {   
            "name" : "frontdoor",
            "provider": "Microsoft.OperationalInsights",
            "general_query" : "AzureDiagnostics%20%7C%20where%20ResourceProvider%20==%20%22MICROSOFT.NETWORK%22%20and%20Category%20==%20%22FrontdoorAccessLog%22%20%20and%20",
            "operation" : "%20%7C%20summarize%20AggregatedValue%20=%20round(100%20-%20(toreal(countif(httpStatusCode_s%20startswith%20%275%27%20or%20httpStatusCode_d%3E499)%20%2F%20toreal(count()))%20*%20100)%2C%203)",
            "services" : [
                {
                    "name": "FT Vault",
                    "filter": "(backendHostname_s%20startswith%20%22vault%22%20or%20(backendHostname_s%20startswith%20%22api-%22%20and%20not%20(backendHostname_s%20startswith%20%22api-notifications-%22))%20or%20requestUri_s%20startswith%20%22https://vault%22)"
                },
                {
                    "name": "FT Remote Access",
                    "filter": "backendHostname_s%20startswith%20%22ftra%22"
                },
                {
                    "name": "FT Common",
                    "filter": "(backendHostname_s%20startswith%20%22common%22%20or%20backendHostname_s%20startswith%20%22home%22%20%20or%20backendHostname_s%20startswith%20%22metrics%22%20%20or%20backendHostname_s%20startswith%20%22help%22%20or%20backendHostname_s%20startswith%20%22api-notifications%22)"
                },
                {
                    "name": "FT Edge Manager",
                    "filter": "backendHostname_s%20startswith%20%22eaas%22"
                },
                {
                    "name": "FT Optix",
                    "filter": "backendHostname_s%20startswith%20%22optix%22"
                },
                {
                    "name": "FT DataMosaix",
                    "filter": "(backendHostname_s+startswith+%22dm%22+or+backendHostname_s+startswith+%22datamosaix%22)"
                }
            ]
        }
    ],
